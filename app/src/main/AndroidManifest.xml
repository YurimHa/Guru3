<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.backbone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Backbone">
        <activity android:name=".LockScreenMenuActivity"></activity>
        <activity android:name=".MyQuestionActivity"></activity>
        <activity android:name=".LockedScreenActivity"></activity>
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SavingActivity"
            android:theme="@style/Theme.MyDialog"></activity>
        <activity android:name=".LockedScreenSetActivity"></activity>
        <activity android:name=".LockedScreenOnceActivity"></activity>
    </application>

</manifest>